using UnityEngine;
using UnityEngine.SceneManagement;

public class MainMenuManager : MonoBehaviour
{
    public void OnStartGame()
    {
        SceneManager.LoadScene("GameScene");
    }

    public void OnUpgrade()
    {
        Debug.Log("업그레이드 버튼 클릭됨");
    }

    public void OnSettings()
    {
        Debug.Log("설정 버튼 클릭됨");
    }
}
